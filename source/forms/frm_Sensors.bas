        '"No deployment info" selected
        If DeploymentNoData Then
            FilterString = "[qfrm_SensorMostRecentActivity].[mostRecentVisitDate] Is Null or (*) or (*) or (*)"
        '"No deployment info" deselected
        Else
            FilterString = "[qfrm_SensorMostRecentActivity].[mostRecentVisitDate] Is Not Null and ((*) or (*) or (*))"
        End If
        '"Successful retrieval" selected
        If RetrievalSuccess Then
            FilterString = Replace(FilterString, "(*)", "([qfrm_SensorMostRecentActivity].[sensorRetrieved] = 'Y')", , 1)
        End If
        '"Failed retrieval" selected
        If RetrievalFailure Then
            FilterString = Replace(FilterString, "*", "[qfrm_SensorMostRecentActivity].[sensorRetrieved] = 'N'", , 1)
        End If
        '"No retrieval info" selected
        If RetrievalNoData Then
            FilterString = Replace(FilterString, "*", "[qfrm_SensorMostRecentActivity].[sensorRetrieved] Is Null", , 1)
        End If
        'get rid of any extra placeholders in filter string
        FilterString = Replace(FilterString, " or (*)", "")
        'turn on filter and return true
        Me.sfrmSensorList.Form.filter = FilterString
        Me.sfrmSensorList.Form.FilterOn = True
        filterSensorList = True
    End If
    
Me.sfrmSensorList.SetFocus
    
End Function

Function selectAll()

Me.chkDeployed = True
Me.chkNotDeployed = True
Me.chkRetrievalSuccess = True
Me.chkRetrievalFailure = True
Me.chkRetrievalNoData = True

End Function

Private Sub chkDeployed_AfterUpdate()

Dim Deployed As Boolean

Deployed = Me.chkDeployed

'"Deployed" selected
If Deployed Then
    'select "Successful retrieval," "Failed retrieval," and "No retrieval info" by default
    Me.chkRetrievalSuccess = True
    Me.chkRetrievalFailure = True
    Me.chkRetrievalNoData = True
'"Deployed" deselected
Else
    'deselect all filters
    Me.chkRetrievalSuccess = False
    Me.chkRetrievalFailure = False
    Me.chkRetrievalNoData = False
End If

'Filter sensor list
filterSensorList

End Sub


