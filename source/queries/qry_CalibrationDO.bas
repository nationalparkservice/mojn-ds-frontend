SELECT TOP 3 data_CalibrationDO_Shared.ID, data_CalibrationDO_Shared.CalibrationDate, data_CalibrationDO_Shared.CalibrationTime, data_CalibrationDO_Shared.PreCalibrationReading_percent, data_CalibrationDO_Shared.PreCalibrationTemperature_C, data_CalibrationDO_Shared.PostCalibrationReading_percent, data_CalibrationDO_Shared.PostCalibrationTemperature_C, data_CalibrationDO_Shared.BarometricPressure_mmHg, data_CalibrationDO_Shared.Notes, data_CalibrationDO_Shared.DateCreated
FROM data_CalibrationDO_Shared
WHERE (((data_CalibrationDO_Shared.CalibrationDate)=Forms!frm_Visit!VisitDate) And ((data_CalibrationDO_Shared.CalibrationTime)<=Forms!frm_Visit!StartTime) And ((data_CalibrationDO_Shared.DOInstrumentID)=Forms!frm_Visit!sfrm_WaterQualityActivity.Form!cboDOInstrumentID)) Or (((data_CalibrationDO_Shared.CalibrationDate)<Forms!frm_Visit!VisitDate) And ((data_CalibrationDO_Shared.DOInstrumentID)=Forms!frm_Visit!sfrm_WaterQualityActivity.Form!cboDOInstrumentID)) Or (((data_CalibrationDO_Shared.CalibrationDate)=Forms!frm_Visit!VisitDate) And ((data_CalibrationDO_Shared.CalibrationTime) Is Null) And ((data_CalibrationDO_Shared.DOInstrumentID)=Forms!frm_Visit!sfrm_WaterQualityActivity.Form!cboDOInstrumentID)) Or (((data_CalibrationDO_Shared.CalibrationDate)<Forms!frm_Visit!VisitDate) And ((data_CalibrationDO_Shared.CalibrationTime) Is Null) And ((data_CalibrationDO_Shared.DOInstrumentID)=Forms!frm_Visit!sfrm_WaterQualityActivity.Form!cboDOInstrumentID))
ORDER BY data_CalibrationDO_Shared.CalibrationDate DESC , data_CalibrationDO_Shared.CalibrationTime DESC;

