'------------------------------------------
' Lock/unlock record based on cbIsLocked
'------------------------------------------
Public Sub VisitLock(bLocked As Boolean)
On Error GoTo ErrorHandler

    Dim ctl As Control
    Dim ctlSub As Control
    
    For Each ctl In Me.Controls
        If ctl.Name <> "cmdLockVisit" And _
            ctl.Name <> "txtSiteName" And _
            ctl.Name <> "txtVisitDate" And _
            ctl.Name <> "txtStartTime" And _
            ctl.Name <> "cmdClose" And _
            ctl.Name <> "cmdViewPhotos" And _
            ctl.Name <> "cmdFieldDataSheet" And _
            ctl.Name <> "cmdGoogleMaps" And _
            ctl.Name <> "cmdSiteInfoSheet" _
            Then
            'Lock all other controls when record is locked
            If ctl.ControlType = acComboBox Or ctl.ControlType = acTextBox Or ctl.ControlType = acCheckBox Or _
                ctl.ControlType = acSubform Then
                    ctl.Locked = bLocked
            End If

            'Disable command buttons when record is locked
            If ctl.ControlType = acCommandButton Then
                ctl.Enabled = Not (bLocked)
            End If
        End If
    Next ctl
    
    'Update Lock Display on form
    cmdLockVisit.Caption = IIf(bLocked = True, "Locked", "Unlocked")
    cmdLockVisit.Picture = IIf(bLocked = True, "Locked-32-Maroon", "Unlocked-32-DkGray")
    
ExitSub:
    Exit Sub
ErrorHandler:
    MsgBox "Error #" & Err.Number & ". " & Err.Description, vbCritical, "Locking error"
    Resume ExitSub
End Sub

