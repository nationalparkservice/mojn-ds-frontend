Private Sub chkRetrievalFailure_AfterUpdate()

If Me.chkRetrievalFailure Then
    Me.chkDeployed = True
ElseIf Not (Me.chkRetrievalSuccess Or Me.chkRetrievalFailure Or Me.chkRetrievalNoData) Then
    Me.chkDeployed = False
