SELECT TOP 3 data_CalibrationSpCond_Shared.ID, data_CalibrationSpCond_Shared.CalibrationTime, data_CalibrationSpCond_Shared.CalibrationDate, data_CalibrationSpCond_Shared.StandardValue_microS_per_cm, data_CalibrationSpCond_Shared.PreCalibrationReading_microS_per_cm, data_CalibrationSpCond_Shared.PostCalibrationReading_microS_per_cm, data_CalibrationSpCond_Shared.SpCondInstrumentID, data_CalibrationSpCond_Shared.Notes, data_CalibrationSpCond_Shared.DateCreated
FROM data_CalibrationSpCond_Shared
WHERE (((data_CalibrationSpCond_Shared.CalibrationDate)=Forms!frm_Visit!VisitDate) And ((data_CalibrationSpCond_Shared.SpCondInstrumentID)=Forms!frm_Visit!sfrm_WaterQualityActivity.Form!cboSpCondInstrumentID) And ((data_CalibrationSpCond_Shared.CalibrationTime)<=Forms!frm_Visit!StartTime)) Or (((data_CalibrationSpCond_Shared.CalibrationDate)<Forms!frm_Visit!VisitDate) And ((data_CalibrationSpCond_Shared.SpCondInstrumentID)=Forms!frm_Visit!sfrm_WaterQualityActivity.Form!cboSpCondInstrumentID)) Or (((data_CalibrationSpCond_Shared.CalibrationDate)=Forms!frm_Visit!VisitDate) And ((data_CalibrationSpCond_Shared.SpCondInstrumentID)=Forms!frm_Visit!sfrm_WaterQualityActivity.Form!cboSpCondInstrumentID) And ((data_CalibrationSpCond_Shared.CalibrationTime) Is Null)) Or (((data_CalibrationSpCond_Shared.CalibrationDate)<Forms!frm_Visit!VisitDate) And ((data_CalibrationSpCond_Shared.SpCondInstrumentID)=Forms!frm_Visit!sfrm_WaterQualityActivity.Form!cboSpCondInstrumentID) And ((data_CalibrationSpCond_Shared.CalibrationTime) Is Null))
ORDER BY data_CalibrationSpCond_Shared.CalibrationDate DESC , data_CalibrationSpCond_Shared.CalibrationTime DESC;

