SELECT data_CalibrationDO.ID, data_CalibrationDO.CalibrationDate, data_CalibrationDO.CalibrationTime, data_CalibrationDO.PreCalibrationReading_percent, data_CalibrationDO.PreCalibrationTemperature_C, data_CalibrationDO.PostCalibrationReading_percent, data_CalibrationDO.PostCalibrationTemperature_C, data_CalibrationDO.BarometricPressure_mmHg, data_CalibrationDO.Notes, data_CalibrationDO.DateCreated
FROM data_CalibrationDO
WHERE (((data_CalibrationDO.CalibrationDate)=Forms!frm_Visit!VisitDate) And ((data_CalibrationDO.CalibrationTime)<=Forms!frm_Visit!StartTime) And ((data_CalibrationDO.DOInstrumentID)=Forms!frm_Visit!sfrm_WaterQualityActivity.Form!cboDOInstrumentID)) Or (((data_CalibrationDO.CalibrationDate)<Forms!frm_Visit!VisitDate) And ((data_CalibrationDO.DOInstrumentID)=Forms!frm_Visit!sfrm_WaterQualityActivity.Form!cboDOInstrumentID)) Or (((data_CalibrationDO.CalibrationDate)=Forms!frm_Visit!VisitDate) And ((data_CalibrationDO.CalibrationTime) Is Null) And ((data_CalibrationDO.DOInstrumentID)=Forms!frm_Visit!sfrm_WaterQualityActivity.Form!cboDOInstrumentID))
ORDER BY data_CalibrationDO.CalibrationDate DESC , data_CalibrationDO.CalibrationTime DESC;

