Private Sub cmdDeleteRepeatPhotoObs_Click()
    
    'Delete Repeat Photo observation record and photo file #s, associated with a visit, from data_RepeatPhotoObservation, and data_RepeatPhoto
    
    On Error Resume Next
    
    Dim YesNo As Integer
    Dim RepeatPhotoObservationsExists As Boolean
    
    
    If IsNull(Me.ID) Then
        Resume Next
    'If user clicks delete button and there are unsaved changes, save the record and then prompt the user to indicate if they're sure they want to get rid of the record.
    Else
        If Not IsNull(Me.ID) And Me.Dirty = True Then
            DoCmd.RunCommand acCmdSaveRecord
            YesNo = MsgBox("You are about to delete this Repeat Photo observation, which may include Photo File #s." & Chr(13) + vbNewLine _
            & "If you click Yes, you won't be able to undo this Delete operation." & Chr(13) _
                & "Are you sure you want to delete this record?", vbYesNo + vbExclamation, "Delete Repeat Photo Observation?")
                    If YesNo = vbYes Then
                        CurrentDb.Execute "Delete * from data_RepeatPhotoObservation where ID = " & Me.ID, dbSeeChanges
                        Me.Requery
                        RepeatPhotoObservationsExists = CheckRecExists(Me.Recordset, "RepeatPhotoActivityID = " & Me.Parent.VisitID)
                            If RepeatPhotoObservationsExists Then
                                Me.sfrmRepeatPhoto.Enabled = True
                            Else
                                Me.sfrmRepeatPhoto.Enabled = False
                                Me.Requery
                            End If
                    Else
                        Me.Undo
                    End If
        Else
            YesNo = MsgBox("You are about to delete this Repeat Photo observation, which may include Photo File #s." & Chr(13) + vbNewLine _
            & "If you click Yes, you won't be able to undo this Delete operation." & Chr(13) _
            & "Are you sure you want to delete this record?", vbYesNo + vbExclamation, "Delete Repeat Photo Observation?")
                If YesNo = vbYes Then
                    CurrentDb.Execute "Delete * from data_RepeatPhotoObservation where ID = " & Me.ID, dbSeeChanges
                    Me.Requery
                    RepeatPhotoObservationsExists = CheckRecExists(Me.Recordset, "RepeatPhotoActivityID = " & Me.Parent.VisitID)
                        If RepeatPhotoObservationsExists Then
                            Me.sfrmRepeatPhoto.Enabled = True
                        Else
                            Me.sfrmRepeatPhoto.Enabled = False
                            Me.Requery
                        End If
                Else
                    Me.Undo
                End If
        End If
    End If
    
End Sub

Private Sub Form_AfterUpdate()

On Error GoTo Error_Handler

    Dim intRepeatPhotoObservationsCount As Integer
    
    intRepeatPhotoObservationsCount = DCount("ID", "data_RepeatPhotoObservation", "RepeatPhotoActivityID = " & Me.Parent.VisitID)
        
    'Enable Repeat Photo form when Repeat Photo Observations are present
    If intRepeatPhotoObservationsCount <> 0 Then
        Me.sfrmRepeatPhoto.Enabled = True
    Else
        Me.sfrmRepeatPhoto.Enabled = False
    End If

Exit_Sub:
    Exit Sub
    
Error_Handler:
    MsgBox "Form: " & mstrcFormName & vbNewLine & "Sub:  Form_AfterUpdate" & vbNewLine & "Error #" & Err.Number & ": " & Err.Description, vbCritical
    Resume Exit_Sub

End Sub

Private Sub Form_BeforeUpdate(Cancel As Integer)

On Error GoTo Error_Handler

    'LC 10/17/2017 - Return a message for any required fields that do not have an entry, or if entries are not within
    'appropriate ranges:
    'UTMX must be between 200000 and 900000.
    'UTMY must be between 3500000 and 4350000.
    'RepeatPhotoTypeID is required.
    
    If IsNull(Me.cboRepeatPhotoTypeID) Then
        MsgBox ("Photo Type is a required field"), vbOKOnly + vbExclamation, "Photo Type"
        Me.txtUtmX_mRepeatPhotos.SetFocus
        Me.cboRepeatPhotoTypeID.SetFocus
    ElseIf Me.txtUtmX_mRepeatPhotos < 200000 Or Me.txtUtmX_mRepeatPhotos > 900000 Then
        MsgBox ("Please re-enter UTMX coordinate within accepted range"), vbOKOnly + vbExclamation, "UTMX Coordinate"
        Me.cboRepeatPhotoTypeID.SetFocus
        Me.txtUtmX_mRepeatPhotos.SetFocus
    ElseIf Me.txtUtmY_mRepeatPhotos < 3500000 Or Me.txtUtmY_mRepeatPhotos > 4350000 Then
        MsgBox ("Please re-enter UTMY coordinate within accepted range"), vbOKOnly + vbExclamation, "UTMY Coordinate"
        Me.txtUtmX_mRepeatPhotos.SetFocus
        Me.txtUtmY_mRepeatPhotos.SetFocus
    End If
    
Exit_Sub:
    Exit Sub
    
Error_Handler:
    MsgBox "Form: " & mstrcFormName & vbNewLine & "Sub:  Form_BeforeUpdate" & vbNewLine & "Error #" & Err.Number & ": " & Err.Description, vbCritical
    Resume Exit_Sub

End Sub
