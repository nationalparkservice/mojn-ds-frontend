WHERE (((data_CalibrationpH_Shared.CalibrationDate)=Forms!frm_Visit!VisitDate) And ((data_CalibrationpH_Shared.pHInstrumentID)=Forms!frm_Visit!sfrm_WaterQualityActivity.Form!cbopHInstrumentID) And ((data_CalibrationpH_Shared.CalibrationTime)<=Forms!frm_Visit!StartTime)) Or (((data_CalibrationpH_Shared.CalibrationDate)<Forms!frm_Visit!VisitDate) And ((data_CalibrationpH_Shared.pHInstrumentID)=Forms!frm_Visit!sfrm_WaterQualityActivity.Form!cbopHInstrumentID)) Or (((data_CalibrationpH_Shared.CalibrationDate)=Forms!frm_Visit!VisitDate) And ((data_CalibrationpH_Shared.pHInstrumentID)=Forms!frm_Visit!sfrm_WaterQualityActivity.Form!cbopHInstrumentID) And ((data_CalibrationpH_Shared.CalibrationTime) Is Null)) Or (((data_CalibrationpH_Shared.CalibrationDate)<Forms!frm_Visit!VisitDate) And ((data_CalibrationpH_Shared.pHInstrumentID)=Forms!frm_Visit!sfrm_WaterQualityActivity.Form!cbopHInstrumentID) And ((data_CalibrationpH_Shared.CalibrationTime) Is Null))
